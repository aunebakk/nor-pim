@model IEnumerable<SolutionNorSolutionPim.BusinessLogicLayer.DefaultUserActivityGroupedContract>

@{
    ViewBag.Title = "Index";
}

<h2>Index for Default User Activity Grouped</h2>

<table class="table">
    <tr>
        <th>Originating Address</th>
        <th>User Activity Type Name</th>
        <th>User Activity Note</th>
        <th>Occurrences</th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.OriginatingAddress) </td>
            <td>@Html.DisplayFor(modelItem => item.UserActivityTypeName) </td>
            <td>@Html.DisplayFor(modelItem => item.UserActivityNote) </td>
            <td>@Html.DisplayFor(modelItem => item.Occurrences) </td>
            <td>
                <a href=@Html.Raw("http://www.whois.com/whois/" + item.OriginatingAddress) } target="_blank">whois</a>
            </td>
        </tr>
    }

</table>
